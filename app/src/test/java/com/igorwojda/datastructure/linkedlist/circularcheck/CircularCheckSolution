import com.igorwojda.datastructure.linkedlist.base.LinkedListSolutionExtended

private object CircularCheckSolution {
    private fun circularCheck(list: LinkedListSolutionExtended.LinkedList<Char>): Boolean {
        var slow = list.first
        var fast = list.head

        list.forEach {
            slow = slow?.next
            fast = fast?.next?.next

            if (fast == null) {
                return false
            }

            if (slow == fast) {
                return true
            }
        }

        return false
    }
}